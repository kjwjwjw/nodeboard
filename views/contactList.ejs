<%-include('header.ejs')%>
    <h1>문의 받은 내용들</h1>

    <table border=1>
        <tr>
            <th>idx</th>
            <th>성명</th>
            <th>번호</th>
            <th>이메일</th>
            <th>문의내용</th>
            <th>삭제</th>
         </tr>


         <% lists.forEach(function(item) { %>
            <tr>
                <td><%=item.idx%></td>
                <td><%=item.name%></td>
                <td><%=item.phone%></td>
                <td><%=item.email%></td>
                <td><%=item.memo%></td>
                <td> <a href="/contactDelete?idx=<%=item.idx%>">삭제</a></td>
             </tr>


         <%}) %>

    </table>

    <%-include('footer.ejs')%>
